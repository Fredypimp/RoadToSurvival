          <argument>
            <kind>1</kind>
            <string>///Selection

// Transforme les data d'objControl en variable locale pour les travailler
var debutX, lastX, debutY, lastY, temp;
debutX = objControl.debutX;
lastX = objControl.lastX;
debutY = objControl.debutY;
lastY = objControl.lastY;
// Des checks pour inverser les variables si la selection est faite en sens inverse
if (debutX &gt; lastX)
{
    temp = debutX;
    debutX = lastX;
    lastX = temp;
}
if (debutY &gt; lastY)
{
    temp = debutY;
    debutY = lastY;
    lastY = temp;
}
    
// Verifie si l'uniter est dans la zone de selection, si oui, la selectionne
if (x &lt;= lastX &amp;&amp; x &gt;= debutX &amp;&amp; y &lt;= lastY &amp;&amp; y &gt;= debutY)
{
    selected = true;
}
else
{
    selected = false;
}
</string>
